<?xml version="1.0" encoding="UTF-8"?>

<openerp>
	<data>
		<record model="ir.ui.view" id="confirmation_wizard_form_view">
			<field name="name">confirmation.form</field>
			<field name="model">confirmation.wizard</field>
			<field name="arch" type="xml">
				<form string="Confirmation wizard Form">
					<div class='col-md-8'>
						Are you sure want to see?
					</div>
					<footer>
						<button name="action_yes" type="object" string="Yes" class="oe_highlight" />
						<button string="No" special="cancel" class="oe_link" />
					</footer>
				</form>
			</field>
		</record>
	</data>
</openerp>
